{
  "product": "{{ product }}",
  "total_questions": {{ faqs|length }},
  "faqs": [
    {%- for faq in faqs %}
    {
      "id": {{ loop.index }},
      "question": "{{ faq.question }}",
      "answer": "{{ faq.answer }}",
      "category": "{{ faq.category }}"
    }{% if not loop.last %},{% endif %}
    {%- endfor %}
  ],
  "generated_at": "{{ generated_at }}"
}
